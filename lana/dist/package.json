{
  "name": "lana",
  "displayName": "Apex Log Analyzer for Salesforce",
  "version": "1.3.1",
  "description": "Debug Log Profile Analyzer",
  "scripts": {},
  "main": "boot.js",
  "icon": "logo.png",
  "publisher": "FinancialForce",
  "author": "",
  "license": "",
  "activationEvents": [
    "onCommand:lana.loadLogFile",
    "onCommand:lana.showLogFile"
  ],
  "engines": {
    "vscode": "^1.41.0"
  },
  "dependencies": {},
  "contributes": {
    "commands": [
      {
        "command": "lana.loadLogFile",
        "title": "Log: Load Apex Log For Analysis"
      },
      {
        "command": "lana.showLogFile",
        "title": "Log: Show Log Analysis"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "lana.showLogFile",
          "when": "resourceExtname == '.log'"
        }
      ],
      "editor/context": [
        {
          "command": "lana.showLogFile",
          "when": "resourceExtname == '.log'"
        }
      ],
      "explorer/context": [
        {
          "command": "lana.showLogFile",
          "when": "resourceExtname == '.log'"
        }
      ]
    }
  }
}
